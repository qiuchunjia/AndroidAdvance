<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--自定义菜单需要的属性-->
    <attr name="firstColor" format="color"></attr>
    <attr name="sencondColor" format="color"></attr>
    <attr name="cicleWidth" format="dimension"></attr>
    <attr name="dotCount" format="integer"></attr>
    <attr name="splitSize" format="integer"></attr>
    <attr name="position">
        <enum name="left_top" value="0" />
        <enum name="right_top" value="1" />
        <enum name="right_bottom" value="2" />
        <enum name="left_bottom" value="3" />
    </attr>
    <attr name="radius" format="dimension"></attr>
    <declare-styleable name="ArcMenu">
        <attr name="position" />
        <attr name="radius" />
    </declare-styleable>
    <!--自定义手势密码需要的属性-->
    <attr name="color_no_finger_inner_circle" format="color" />
    <attr name="color_no_finger_outer_circle" format="color" />
    <attr name="color_finger_on" format="color" />
    <attr name="color_finger_up" format="color" />
    <attr name="count" format="integer" />
    <attr name="tryTimes" format="integer" />

    <declare-styleable name="GestureLockViewGroup">
        <attr name="color_no_finger_inner_circle" />
        <attr name="color_no_finger_outer_circle" />
        <attr name="color_finger_on" />
        <attr name="color_finger_up" />
        <attr name="count" />
        <attr name="tryTimes" />
    </declare-styleable>
    <!--微信截图属性-->
    <attr name="border_color" format="color" />
    <attr name="border_out_color" format="color" />
    <attr name="horizontalPadding" format="dimension" />
    <declare-styleable name="ClipImageLayout">
        <attr name="border_color" />
        <attr name="border_out_color" />
        <attr name="horizontalPadding" />
    </declare-styleable>
    <!--viewpagerindicator-->
    <attr name="visiable_item_count" format="integer" />
    <declare-styleable name="viewpagerindicator">
        <attr name="visiable_item_count" />
    </declare-styleable>
    s
</resources>